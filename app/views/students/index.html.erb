<h1>All Students</h1>
<ol>
  <% @students.each do |student| %>
    <section id="student-<%= "#{student.id}" %>">
      <li><%= "#{student.name}" %></li>
      <p>Cohort: <%= "#{student.cohort}" %></p>
      <ul>
        <p>Instructors:</p>
        <% student.instructors.each do |instructor| %>
        <li><%=link_to instructor.name, "/instructors/#{instructor.id}" %></li>
        <% end %>
      </ul>
    </section>
  <% end %>
</ol>
